        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="60" Margin="554,155,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="320" FontSize="36"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="60" Margin="554,259,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="320" FontSize="36"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="60" Margin="554,353,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="320" FontSize="36"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="60" Margin="554,443,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="320" FontSize="36"/>

    </Grid>
