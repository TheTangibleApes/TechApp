<Window x:Class="TechApp.CompleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TechApp"
        mc:Ignorable="d"
        Title="CompleteWindow" Height="720" Width="1280">
    <Window.Background>
        <ImageBrush ImageSource="BGImage.png"/>
    </Window.Background>
    <Grid Margin="0,0,-8,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1153*"/>
            <ColumnDefinition Width="127*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="TextBox1" HorizontalAlignment="Left" Height="112" Margin="62,219,0,0" TextWrapping="Wrap" Text="Thank you for completing." VerticalAlignment="Top" Width="1065" FontSize="72" Foreground="White" FontStyle="Italic"/>
        <Button x:Name="button" Content="" HorizontalAlignment="Left" Height="77" Margin="62,521,0,0" VerticalAlignment="Top" Width="302" BorderBrush="#00707070">
            <Button.Background>
                <ImageBrush ImageSource="button-next.png"/>
            </Button.Background>
        </Button>
        <Grid HorizontalAlignment="Left" Height="100" Margin="61.114,198,-34,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="379" Source="Tech_LOGO.png"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="38" Margin="88,483,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="268" FontSize="24" FontWeight="Bold" FontStyle="Italic" Foreground="White"/>

    </Grid>
</Window>
